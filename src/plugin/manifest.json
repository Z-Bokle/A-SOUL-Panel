{
    "name":"A-SOUL Panel",
    "version":"0.1",
    "manifest_version":3,
    "description":"New Tab for A-SOUL.",
    "icons":{
        "16":"assets/icons/16.png",
        "32":"assets/icons/32.png",
        "48":"assets/icons/48.png",
        "128":"assets/icons/128.png"
    },
    "host_permissions":[
        "http://*/",
        "https://*/"
    ],
    "permissions":["storage","alarms","notifications","tabs"],
    "homepage_url":"https://github.com/Z-Bokle/A-SOUL-Panel",
    "chrome_url_overrides":{
        "newtab":"tab.html"
    },
    "background":{
        "service_worker":"service_worker.js"
    },
    "action":{
        "default_icon":"assets/icons/32.png",
        "default_title":"关注嘉然，顿顿解馋！😋"
    }
}